,\\\"jollyany_event_end\\\":\\\"\\\",\\\"jollyany_event_expired\\\":\\\"\\\",\\\"jollyany_event_spot\\\":\\\"\\\",\\\"jollyany_event_url\\\":\\\"\\\",\\\"jollyany_event_url_text\\\":\\\"\\\",\\\"jollyany_event_long_lat\\\":\\\"\\\",\\\"event_multi_locations\\\":[]}\",\"version\":1,\"ordering\":null,\"metakey\":\"\",\"metadesc\":\"\",\"access\":1,\"hits\":0,\"metadata\":\"{\\\"robots\\\":\\\"\\\",\\\"author\\\":\\\"\\\",\\\"rights\\\":\\\"\\\"}\",\"featured\":\"0\",\"language\":\"*\",\"note\":\"\"}', '0');/**ABDB**/
